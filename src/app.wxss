@import './resetcss/reset.wxss';
/**app.wxss**/
page {
    height: 100%;
}
view, text {
    -webkit-text-size-adjust: none;
    color: #343434;
    font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,微软雅黑,Arial,sans-serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

/* common */
.line-bottom {
    position: relative;
}
.line-bottom:after {
    content: '';
    display: block;
    height: 1rpx;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #ddd;
}

.clearfix:after, .clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
  visibility: hidden;
  font-size: 0;
  height: 0;
}
@media screen and (max-width:321px){
    .font-title{font-size:16px}
    .font-subhead{font-size:15px;}
    .font-wide{font-size:14px;}
    .font-normal{font-size:12px;}
    body{font-size:12px;}
}

@media screen and (min-width:321px) and (max-width:400px){
    .font-title{font-size:18px}
    .font-subhead{font-size:17px;}
    .font-wide{font-size:16px;}
    .font-normal{font-size:14px;}
    body{font-size:14px;}
}

@media screen and (min-width:400px){
    .font-title{font-size:20px}
    .font-subhead{font-size:19px;}
    .font-wide{font-size:18px;}
    .font-normal{font-size:16px;}
    body{font-size:16px;}
}

