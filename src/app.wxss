/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}


/* common */
.line-bottom {
    position: relative;
}
.line-bottom:after {
    content: '';
    display: block;
    height: 1rpx;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #ddd;
}

.clearfix:after, .clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
  visibility: hidden;
  font-size: 0;
  height: 0;
}

/* 消息提示样式*/
.message-prompts {
    background-color: rgba(0,0,0,.4);
    color: #fff;
    position:fixed;
    bottom: 300rpx;
    text-align: center;
    padding: 20rpx;
    display: block;
    margin:auto;
    border-radius: 40rpx;
    font-size: 32rpx;
    right: 0;
    left: 0;
    max-width: 500rpx;
}


