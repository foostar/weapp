    height: 32rpx;
    border-radius: 6rpx;
    margin-right: 10rpx;
    display: inline-block;
    vertical-align: middle;
}
.icon-tag-ding {
    background-color: #F70505;
}
.icon-tag-jing {
    background-color: #0BBDA9;
}
.icon-tag-re {
    background-color: #F78005;
}
.icon-tag-ding:after, .icon-tag-jing:after, .icon-tag-re:after {
    display: block;
    text-align: center;
    font-size: 18rpx;
    line-height: 34rpx;
}
.icon-tag-ding:after {
    content: '顶';
}
.icon-tag-jing:after {
    content: '精';
}
.icon-tag-re:after {
    content: '热';
}
