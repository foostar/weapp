<view class="layout1col1row">
    <view class="layout-1-col-1-row-item {{style}} {{item.iconStyle}}" bindtap="layout1col1row_touch" data-index="{{ index }}" data-event-key="{{key}}" wx:for="{{componentList}}">
        <image mode="{{item.iconStyle == 'circle' ? 'aspectFill' : 'aspectFit' }}" src="{{item.icon}}"></image>
        <text>{{ item.desc }}</text>
    </view>
</view>