<!-- cell-tieba -->
<template name="tieba">
    <view class="cell-tieba" bindtap="clickItem" id="{{id}}">
        <view class="cell-tieba-header">
            <view>
                <image class="cell-tieba-header-avatar" src="{{userAvatar}}" />
                <view class="cell-tieba-header-meta">
                    <text>{{user_nick_name}}</text>
                    <text class="cell-tieba-header-time">{{last_reply_date}}</text>
                </view>
            </view>
            <view class="cell-tieba-title">{{title}}</view>
        </view>
        <view class="cell-tieba-content">{{subject}}</view>
        <view class="cell-tieba-thumb" wx:if="{{imageList.length > 0}}">
            <block wx:for="{{imageList}}">
                <view wx:if="{{index < 3}}" class="cell-tieba-thumb-container">
                    <image src="{{item}}" mode="aspectFill"></image>
                </view>
            </block>
        </view>
        <view class="cell-tieba-status">
            <text class="hits">{{hits}}</text>
            <text class="replies">{{replies}}</text>
        </view>
    </view>
</template>
<!-- cell-flat -->
<template name="flat">
    <view class="cell-flat">
        <view class="cell-flat-title">{{title}}</view>
        <view class="cell-flat-content">
            <block wx:if="{{imageList.length === 1}}">
                <view class="cell-flat-thumb-container-1">
                    <image src="{{imageList[ 0 ]}}" mode="aspectFill"></image>
                </view>
                <view class="cell-flat-content-subject-1">{{subject}}</view>
            </block>
            <block wx:elif="{{imageList.length > 1}}}">
                <view class="cell-flat-content-subject">{{subject}}</view>
                <view class="cell-flat-thumb">
                    <block wx:for="{{imageList}}">
                        <view wx:if="{{index < 3}}" class="cell-flat-thumb-container">
                            <image src="{{item}}" mode="aspectFill"></image>
                        </view>
                    </block>
                </view>
            </block>
        </view>
        <view class="cell-flat-info">
            <text class="cell-flat-info-time">{{last_reply_date}}</text>
            <text class="cell-flat-info-author">{{user_nick_name}}</text>
            <text class="hits">{{hits}}阅读</text>
        </view>
    </view>
</template>
<!-- card -->
<template name="card">
    <view class="cell-card" bindtap="clickItem" id="{{id}}">
        <view class="cell-card-header">
            <view class="cell-card-user">
                <image class="cell-card-header-avatar" src="{{userAvatar}}" />
                <view class="cell-card-header-meta font-wide">
                    <view class="cell-card-header-name">{{user_nick_name}}</view>
                    <view class="cell-card-header-time font-mini">{{last_reply_date}}</view>
                </view>
                <block wx:for="{{verify}}">
                    <image wx:if="{{index < 3}}" class="cell-card-verify" src="{{item.icon}}"></image>
                </block>
            </view>
            <view class="cell-card-from font-normal">
                来自:
                <navigator class="text-color">{{board_name}}</navigator>
            </view>
        </view>
        <view class="cell-card-content">
            <view class="cell-card-title">{{title}}</view>
            <view class="cell-card-subject font-normal">{{subject}}asdfasfasdfasf</view>
        </view>
        <view class="cell-card-meta font-mini">
            <view class="hits"><image src="data:image/png;base64,"/>{{hits}}</view>
            <view class="replies"><image src="data:image/png;base64,"/>{{replies}}</view>
        </view>
    </view>
</template>
<!--imageSudoku-->
<template name="imageSudoku">
    <view class="cell-imageSudoku-wrapper" bindtap="clickItem" id="{{id}}">
        <view class="cell-imageSudoku">
            <image class="cell-imageSudoku-avatar" src="{{userAvatar}}" />
            <view class="cell-imageSudoku-content font-normal">
                <view class="cell-imageSudoku-author">
                    <view class="cell-imageSudoku-username">{{user_nick_name}}</view>
                    <block wx:for="{{verify}}">
                        <image wx:if="{{index < 3}}" class="cell-imageSudoku-verify" src="{{item.icon}}"></image>
                    </block>
                    <view class="cell-imageSudoku-from font-mini">
                        来自:
                        <navigator class="text-color">{{board_name}}</navigator>
                    </view>
                </view>
                <view class="cell-imageSudoku-subject">
                    <view wx:if="{{item.top == 1}}" class="icon-tag-ding"></view>
                    {{subject}}
                </view>
                <view class="cell-imageSudoku-mes font-mini">
                    <view class="cell-imageSudoku-date color-gray">{{last_reply_date}}</view>
                    <view class="cell-imageSudoku-meta">
                        <view class="cell-imageSudoku-like color-gray">
                            <image src="data:image/png;base64,"/>
                            {{recommendAdd}}
                        </view>
                        <view class="cell-imageSudoku-replies color-gray">
                        <image src="data:image/png;base64,"/>
                        {{replies}}</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</template>
<!--circle-->
<template name="circle">
    <view class="cell-wrapper" bindtap="clickItem" id="{{id}}">
        <view class="cell-container cell-flex-container">
            <image class="cell-avatar" src="{{userAvatar}}" />
            <view class="cell-circle-content font-normal">
                <view class="cell-circle-author">
                    <view class="cell-circle-username">
                        <view class="cell-circle-name">{{user_nick_name}}</view>
                        <view class="cell-circle-date font-mini color-gray">{{last_reply_date}}</view>
                    </view>
                    <block wx:for="{{verify}}">
                        <image wx:if="{{index < 3}}" class="cell-verify" src="{{item.icon}}"></image>
                    </block>
                    <view class="cell-from font-mini">
                        来自:
                        <navigator class="text-color">{{board_name}}</navigator>
                    </view>
                </view>
                <view class="cell-circle-subject-wrapper">
                    <view wx:if="{{item.top == 1}}" class="icon-tag-ding"></view>
                    <view class="cell-circle-title">{{title}}</view>
                    <view class="cell-circle-subject font-mini color-gray">{{subject}}</view>
                </view>
                <view class="cell-circle-mes font-mini">
                    <view class="cell-imageSudoku-like color-gray">
                        <image src="data:image/png;base64,"/>
                    </view>
                    <view class="cell-imageSudoku-replies color-gray">
                    <image src="data:image/png;base64,"/></view>
                </view>
                <view wx:if="{{recommendAdd || reply.length > 0}}" class="cell-circle-reply font-mini">
                    <view wx:if="{{recommendAdd}}" class="cell-circle-reply-like font-mini">
                        <view class="cell-circle-reply-recommendAdd"><image src="data:image/png;base64,"/>{{recommendAdd}}</view>
                        <block wx:for="{{zanList}}">
                            <view class="cell-circle-likemember">{{item.username}}{{index < zanList.length-1 ? ',' : ''}}</view>
                        </block>
                    </view>
                    <view class="cell-circle-reply-main font-mini">
                        <block wx:for="{{reply}}" wx:if="{{!!reply}}">
                            <view class="cell-circle-reply-box" wx:if="{{index < 6}}">
                                <view class="cell-circle-reply-author">{{item.username}}:</view>
                                <view class="cell-circle-reply-content">{{item.text}}</view>
                            </view>
                        </block>
                    </view>
                    <view wx:if="{{replies > 6}}" class="cell-circle-reply-more font-mini">查看全部评论({{replies}})</view>
                </view>
            </view>
        </view>
    </view>
</template>