.tabbar {
    height: 88rpx;
    width: 100%;
    flex-direction: row;
    display: flex;
    overflow: hidden;
    bottom: 0;
    border-top: 1rpx solid #CCC;
    background-color: #fff;
}
.tabbar-item {
    flex: 1;
    display: flex;
    position: relative;
}
.tabbar-item-image {
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin: -44rpx;
}
.tabbar-itme-name {
    width: 100%;
    height: 20rpx;
    line-height: 20rpx;
    position: absolute;
    bottom: 2rpx;
    text-align: center;
    font-size: 7rpx;
}