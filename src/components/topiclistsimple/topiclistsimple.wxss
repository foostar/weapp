/* ========== forum-info ========== */
.forum-info-container{
    background:#fff;
    border-bottom:2rpx solid #ddd;
    padding:30rpx;
    box-sizing:border-box;
    display:flex;
    justify-content:space-between;
    
}
.foruminfo-icon,.foruminfo-icon image{
    width:110rpx;
    height:110rpx;
    border-radius:5px;
}
.foruminfo-content{
    flex:1;
    padding-left:25rpx;
}
.foruminfo-forumname{
    color:#000;
    margin-bottom:8rpx;
}
.foruminfo-meta text{
    margin-left:10rpx;
}
.foruminfo-meta,.foruminfo-des{
    line-height:30rpx;
}
.foruminfo-meta,.foruminfo-des,.foruminfo-meta text{
    color:#a4a4a4
}
.foruminfo-handler{
    color:#fff;
    display:inline-block;
    padding:0rpx 20rpx;
    height:65rpx;
    line-height:65rpx;
    border-radius:5px;
    box-sizing:border-box;
    background-color:#a4a4a4
}
.stick-container{
    background:#fff;
}
.stick-item{
    display:flex;
    padding:8rpx 20rpx;
    line-height:45rpx;
    border-bottom:2rpx solid #ddd;
}
.stick-tag{
    padding:0 12rpx;
    color:#fff;
    height:40rpx;
    line-height:42rpx;
    margin-right:20rpx;
    border-radius:5px;
}
.liststyle-container{
    margin-top:30rpx
}
.liststyle-index{
    margin-top:0
}
/* ========== cell-tieba ========== */
.cell-tieba {
    width: 100%;
    background: #fff;
    padding: 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ddd;
}
.cell-tieba-header {
}
.cell-tieba-meta {
    display: flex;
    flex-direction: column;
}
.cell-tieba-header-avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 6rpx;
    float: left;
}
.cell-tieba-header-meta {
    margin-left: 96rpx;
    font-size: 28rpx;
}
.cell-tieba-header-meta text {
    display: block;
    line-height: 40rpx;
}
.cell-tieba-header-time {
    color: #a4a4a4;
    font-size: 24rpx;
}
.cell-tieba-title {
    font-size: 32rpx;
    margin: 20rpx 0;
    width: 100%;
    /*white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;*/
}

.cell-tieba-content {
    font-size: 26rpx;
    color: #a4a4a4;
    width: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    margin: 16rpx 0;
    /*overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;*/
}
.cell-tieba-thumb {
    display: flex;
    justify-content: space-center;
}
.cell-tieba-thumb-container {
    width: 240rpx;
    height: 228rpx;
    margin: 0 6rpx;
    overflow: hidden;
    position: relative;
}
.cell-tieba-thumb image {
    width: 280rpx;
    height: 228rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.cell-tieba-status {
    color: #a4a4a4;
    text-align: right;
    font-size: 24rpx;
    margin-top: 16rpx;
    padding-right: 10rpx;
}
.cell-tieba-status .hits {
    padding-left: 30rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 28rpx auto;
}

.cell-tieba-status .replies {
    padding-left: 34rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 28rpx auto;
    margin-left: 20rpx;
}
/* ========== cell-flat ========== */
.cell-flat {
    background-color: #fff;
    padding: 20rpx;
    border-bottom: 1rpx solid #ddd;
}
.cell-flat-title {
    font-size: 32rpx;
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.cell-flat-content-subject {
    font-size: 26rpx;
    color: #a4a4a4;
    width: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    margin: 16rpx 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.cell-flat-thumb {
    display: flex;
    justify-content: center;
}
.cell-flat-thumb-container {
    width: 228rpx;
    height: 160rpx;
    overflow: hidden;
    position: relative;
    margin: 0 5rpx;
}
.cell-flat-thumb image {
    width: 228rpx;
    height: 160rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.cell-flat-content-subject-1 {
    display: inline-block;
    font-size: 26rpx;
    color: #a4a4a4;
    float: left;
    width: 76%;
    /*white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;*/
    /*margin-right: 154rpx;*/
}
.cell-flat-thumb-container-1 {
    width: 144rpx;
    height: 132rpx;
    float: right;
    position: relative;
}
.cell-flat-thumb-container-1 image{
    width: 144rpx;
    height: 132rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.cell-flat-info {
    font-size: 24rpx;
    color: #a4a4a4;
    margin-top: 16rpx;
}
.cell-flat-info-time {
    width: 210rpx;
    display: inline-block;
}

.cell-flat-info .hits {
    float: right;
}