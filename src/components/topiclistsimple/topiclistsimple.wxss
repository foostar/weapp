/* ========== forum-info ========== */
.cell-wrapper{
    border-bottom:2rpx solid #ddd;
    background:#fff;
}
.cell-container{
    padding:20rpx;
}
.cell-flex-container{
    display:flex;
}
.color-gray{
    color:#a4a4a4;
}
.cell-avatar{
    width:110rpx;
    height:110rpx;
    margin-right:25rpx;
    border-radius:5px;
}
.cell-verify{
    width:75rpx;
    height:26rpx;
    margin-right:1rpx;
}
.cell-from navigator{
    display:inline-block;
}
/* ========== forum-info ========== */
.forum-info-container{
    background:#fff;
    border-bottom:2rpx solid #ddd;
    padding:30rpx;
    box-sizing:border-box;
    display:flex;
    justify-content:space-between;
    
}
.foruminfo-icon,.foruminfo-icon image{
    width:110rpx;
    height:110rpx;
    border-radius:5px;
}
.foruminfo-content{
    flex:1;
    padding-left:25rpx;
}
.foruminfo-forumname{
    color:#000;
    margin-bottom:3rpx;
}
.foruminfo-meta text{
    margin-left:10rpx;
}
.foruminfo-meta,.foruminfo-des{
    line-height:35rpx;
}
.foruminfo-meta,.foruminfo-des,.foruminfo-meta text{
    color:#a4a4a4
}
.foruminfo-handler{
    color:#fff;
    display:inline-block;
    padding:0rpx 20rpx;
    height:65rpx;
    line-height:68rpx;
    border-radius:5px;
    box-sizing:border-box;
    background-color:#a4a4a4
}
.stick-container{
    background:#fff;
}
.stick-item{
    display:flex;
    padding:8rpx 20rpx;
    line-height:45rpx;
    border-bottom:2rpx solid #ddd;
    color:#a4a4a4;
}
.stick{
    color:#a4a4a4;
}
.stick-tag{
    padding:0 12rpx;
    color:#fff;
    height:40rpx;
    line-height:42.5rpx;
    margin-right:20rpx;
    border-radius:5px;
}
.stick-more{
    text-align:center;
    justify-content:center;
}
.liststyle-container{
    margin-top:30rpx
}
.liststyle-index{
    margin-top:0
}
/* ========== cell-tieba ========== */
.cell-tieba {
    width: 100%;
    background: #fff;
    padding: 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ddd;
}
.cell-tieba-header {
}
.cell-tieba-meta {
    display: flex;
    flex-direction: column;
}
.cell-tieba-header-avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 6rpx;
    float: left;
}
.cell-tieba-header-meta {
    margin-left: 96rpx;
    font-size: 28rpx;
}
.cell-tieba-header-meta text {
    display: block;
    line-height: 40rpx;
}
.cell-tieba-header-time {
    color: #a4a4a4;
    font-size: 24rpx;
}
.cell-tieba-title {
    font-size: 32rpx;
    margin: 20rpx 0;
    width: 100%;
    /*white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;*/
}

.cell-tieba-content {
    font-size: 26rpx;
    color: #a4a4a4;
    width: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    margin: 16rpx 0;
    /*overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;*/
}
.cell-tieba-thumb {
    display: flex;
    justify-content: space-center;
}
.cell-tieba-thumb-container {
    width: 240rpx;
    height: 228rpx;
    margin: 0 6rpx;
    overflow: hidden;
    position: relative;
}
.cell-tieba-thumb image {
    width: 280rpx;
    height: 228rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.cell-tieba-status {
    color: #a4a4a4;
    text-align: right;
    font-size: 24rpx;
    margin-top: 16rpx;
    padding-right: 10rpx;
}
.cell-tieba-status .hits {
    padding-left: 30rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 28rpx auto;
}

.cell-tieba-status .replies {
    padding-left: 34rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 28rpx auto;
    margin-left: 20rpx;
}
/* ========== cell-flat ========== */
.cell-flat {
    background-color: #fff;
    padding: 20rpx;
    border-bottom: 1rpx solid #ddd;
}
.cell-flat-title {
    font-size: 32rpx;
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.cell-flat-content-subject {
    font-size: 26rpx;
    color: #a4a4a4;
    width: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    margin: 16rpx 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.cell-flat-thumb {
    display: flex;
    justify-content: center;
}
.cell-flat-thumb-container {
    width: 228rpx;
    height: 160rpx;
    overflow: hidden;
    position: relative;
    margin: 0 5rpx;
}
.cell-flat-thumb image {
    width: 228rpx;
    height: 160rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.cell-flat-content-subject-1 {
    display: inline-block;
    font-size: 26rpx;
    color: #a4a4a4;
    float: left;
    width: 76%;
    /*white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;*/
    /*margin-right: 154rpx;*/
}
.cell-flat-thumb-container-1 {
    width: 144rpx;
    height: 132rpx;
    float: right;
    position: relative;
}
.cell-flat-thumb-container-1 image{
    width: 144rpx;
    height: 132rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.cell-flat-info {
    font-size: 24rpx;
    color: #a4a4a4;
    margin-top: 16rpx;
}
.cell-flat-info-time {
    width: 210rpx;
    display: inline-block;
}

.cell-flat-info .hits {
    float: right;
}
/* ========== cell-card ========== */
.cell-card {
    background-color: #fff;
    margin-top:20rpx;
    border-bottom: 2rpx solid #ddd;
}
.cell-card-header{
    display:flex;
    width:100%;
    padding: 20rpx 20rpx 0 20rpx;
    box-sizing:border-box;
    justify-content:space-between;
}
.cell-card-user{
    display:flex;
}
.cell-card-header-meta{
    width:150rpx;
    margin-right:15rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.cell-card-header-name{
    margin-bottom:5rpx;
    margin-top:0rpx;
}
.cell-card-verify{
    width:75rpx;
    height:26rpx;
    margin-right:8rpx;
    margin-top:10rpx;
}
.cell-card-header-avatar{
    width:80rpx;
    height:80rpx;
    border-radius:5px;
    margin-right:20rpx;
}
.cell-card-header-time{
    color:#a4a4a4;
    line-height:37rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.cell-card-from navigator{
    display:inline-block;
}
.cell-card-subject{
    color:#a4a4a4;
    margin-top:10rpx;
    line-height:35rpx;
}
.cell-card-content{
    padding:18rpx 20rpx;
    border-bottom: 2rpx solid #ddd;
}
.cell-card-meta{
    padding:10rpx 0;
    display:flex;
}
.cell-card-meta view{
    flex:1;
    text-align:center; 
    color:#a4a4a4;
    line-height:30rpx;
    box-sizing: border-box;
}
.cell-card-meta .hits{
    border-right: 2rpx solid #ddd;
}
.cell-card-meta view image{
    width:28rpx;
    height:22rpx;
    margin-right:7rpx;
    height:auto;
    display:inline-block;
}
/* ========== cell-imageSudoku ========== */
.cell-imageSudoku-wrapper{
    border-bottom:2rpx solid #ddd;
    background:#fff;
}
.cell-imageSudoku{
    padding:20rpx;
}
.cell-imageSudoku{
    display:flex;
}
.cell-imageSudoku-content{
    flex:1;
}
.cell-imageSudoku-avatar{
    width:110rpx;
    height:110rpx;
    margin-right:25rpx;
    border-radius:5px;
}
.cell-imageSudoku-author{
    display:flex;
    justify-content: space-between;
}
.cell-imageSudoku-mes{
    display:flex;
    justify-content: space-between;
    color:#a4a4a4;
}
.cell-imageSudoku-from navigator{
    display:inline-block;
}
.cell-imageSudoku-verify{
    width:75rpx;
    height:26rpx;
    margin-right:1rpx;
}
.cell-imageSudoku-subject{
    padding:15rpx 0 20rpx;
}
.cell-imageSudoku-meta{
    display:flex;
    justify-content: space-between;
}
.cell-imageSudoku-meta view image{
    width:28rpx;
    height:22rpx;
    position:relative;
    top:3rpx;
    height:auto;
    display:inline-block;
}
.cell-imageSudoku-like{
    margin-right:12rpx;
}

/* ========== cell-circle ========== */
.cell-circle-author{
    display:flex;
    justify-content: space-between;
    margin-top:8rpx;
}
.cell-circle-mes{
    display:flex;
    justify-content: flex-end;
    margin-bottom:20rpx;
}
.cell-circle-mes view{
    padding:5rpx 20rpx;
    border-radius:10px;
    border:2rpx solid #ddd;
}
.cell-circle-mes view image{
    width:28rpx;
    height:22rpx;
    position:relative;
    top:3rpx;
    height:auto;
    display:inline-block;
}
.cell-circle-subject-wrapper{
    padding:20rpx 0;
}
.cell-circle-title{
    margin-bottom:10rpx;
}
.cell-circle-content{
    flex:1;
}
.cell-circle-name{
    line-height:35rpx;
    margin-bottom:12rpx;
}
.cell-circle-reply{
    background:#f2f2f2;
}
.cell-circle-reply-like,.cell-circle-reply-main{
    padding:12rpx 10rpx;
}
.cell-circle-reply-like{
    border-bottom: 2rpx solid #ddd;
    display:flex;
}
.cell-circle-reply-recommendAdd{
    margin-right:5rpx;
    color:#555;
}
.cell-circle-reply-recommendAdd image{
    width:28rpx;
    height:22rpx;
    position:relative;
    margin-right:5rpx;
    height:auto;
    display:inline-block;
}
.cell-circle-reply-main{
    padding-top:5rpx;
}
.cell-circle-likemember{
    color:#2db7f5;
    margin-left:12rpx;
}
.cell-circle-reply-author{
    color:#2db7f5;
    margin-right:8rpx;
}
.cell-circle-reply-box{
    display:flex;
    padding-top:8rpx;
    line-height:35rpx;
}
.cell-circle-reply-more{
    padding:8rpx 10rpx;
    border-top:2rpx solid #ddd;
}