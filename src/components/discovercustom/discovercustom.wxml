<block wx:for="{{list}}">
    <view class="personal-manage-item" bindtap='discovercustom_toNavigationPage' data-event-key="{{key}}" data-page="myCollection" data-id="{{item.extParams.moduleId}}">
        <image class="icon" src="{{item.icon}}"></image>
        <text class="personal-manage-item-title">{{item.title}}</text>
        <image class="icon-arrow"></image>
    </view>
</block>
