.layout3col {
    width: 100%;
    flex-direction: row;
    display: flex;
    background: #fff;
}
.layout-3-col-item {
    flex: 1;
    display: flex;
}
.layout-3-col-item image {
    width: 100%;
    height: 100%;
    display: block;
}

.layoutThreeCol_Super_Low.textOverlapUp_Video:after, .layoutThreeCol_Super_Low.textOverlapDown_Video:after {
    width: 54rpx;
    height: 54rpx;
    margin-left: -27rpx;
    margin-top: -27rpx;
}

/* 圆 */
.layout-3-col-item.circle {
    display: block;
}
.layout-3-col-item.circle image {
    height: 61.5%;
    margin: 0 auto;
}

/* 默认 */
.layoutdefault .layout-3-col-item {
    margin: 10rpx;
}
.layoutdefault .layout-3-col-item.layoutThreeCol_High {
    height: 291.304348rpx;
}
.layoutdefault .layout-3-col-item.layoutThreeCol_Mid {
    height: 223.333331rpx;   
}
.layoutdefault .layout-3-col-item.layoutThreeCol_Low {
