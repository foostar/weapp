@import "../module/module.wxss";
.viewer {
    height: 100%;
    width: 100%;
}
.viewer-background {
    position: absolute;
    height: 100%;
    width: 100%;
    background-color: #FFF;
    z-index: 0;
}
.copyright {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: -10;
    text-align: center;
    font-size: 20rpx;
    color: #FFF;
    background: #F0F0F0;
    padding: 30rpx;
    margin-top: 40rpx;
}
