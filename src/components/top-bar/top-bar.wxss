.subnavTopbar{
    width:100%;
    height:75rpx;
    position:fixed;
    top:0;
    left:0;
    background:#fff;
    display:flex;
    justify-content:space-between;
    line-height:75rpx;
}
.topbar-icon{
    width:75rpx;
    height:75rpx;
    margin:0 15rpx;
}
.top-bar{
    width:55%;
    margin:0 auto;
    display:flex;
}
.top-bar view{
    flex:1;
    text-align:center;
}
.top-bar .active{
    border-bottom:4.8rpx solid currentColor
}

