.tab-nav-bar {
    width: 100%;
    height: 66rpx;
    flex-direction: row;
    display: flex;
    background: #fff;
    position: relative;
}
.tab-nav-bar-item {
    flex: 1;
    display: flex;
    border-bottom: 1rpx solid #E0E0E0;
}
.tab-nav-bar-item text {
    line-height: 66rpx;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
}

.tabs-indicator {
    position: absolute;
    height: 1rpx;
    bottom: 0;
    z-index: 999;
    transition: .3s;
}

.tabs-indicator-inner {
    height: 100%;
    margin: 0 5%;
}

.subnav-content-container {
    display: flex;
    flex-direction: row;
    transition: .3s;
}
.subnav-content {
    flex: 1;
    overflow: hidden;
    min-height: 1rpx;
}
