.tab-nav-bar {
    width: 100%;
    height: 66rpx;
    flex-direction: row;
    display: flex;
    border-bottom: 1rpx solid #E0E0E0;
    margin-top: 10px;
    background: #fff;
}
.tab-nav-bar-item {
    flex: 1;
    display: flex;
}
.tab-nav-bar-item text {
    line-height: 66rpx;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
}

.tab-nav-bar-item-cur {
    color: #0894ec;
    border-bottom: 2px solid #0894ec;
}
